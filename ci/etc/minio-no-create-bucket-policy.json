{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
          "s3:ListAllMyBuckets",
	  "s3:PutObject",
	  "s3:GetObject",
	  "s3:ListBucket",
	  "s3:PutObjectTagging",
	  "s3:DeleteObject",
	  "s3:GetObjectVersion"
      ],
      "Resource": [
        "arn:aws:s3:::*"
      ]
    }
  ]
}
