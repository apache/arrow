{
  "extends": "../tsconfig.json",
  "include": [
    "../src/**/*.ts",
    "../test/**/*.ts"
  ],
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "importHelpers": false,
    "noEmitHelpers": false,
    "noEmitOnError": false,

    "sourceMap": false,
    "inlineSources": false,
    "inlineSourceMap": false,
    "downlevelIteration": false
  }
}
